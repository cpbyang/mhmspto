@{
    ViewBag.Title = "Calendar";
}
<div class="container">
    <h2>@ViewBag.Title</h2>
    <ul class="nav nav-tabs">
        <li class="active"><a data-toggle="tab" href="#2015"><b>Next Year (2015-2016)</b></a></li>
        <li><a data-toggle="tab" href="#2014"><b>Current Year (2014-2015)</b></a></li>
    </ul>

    <div class="tab-content">
        <div id="2015" class="tab-pane fade in active">
            @*<h3>2015-2016</h3>*@
            <object data="../content/documents/2015_2016_PTO_Calendar.pdf" type="application/pdf" width="100%" height="750">
                alt : <a href="../content/documents/2015_2016_PTO_Calendar.pdf">../content/documents/2015_2016_PTO_Calendar.pdf</a>
            </object>
        </div>
        <div id="2014" class="tab-pane fade">
            @*<h3>2014-2015</h3>*@
            <object data="../content/documents/PTO_Calendar.pdf" type="application/pdf" width="100%" height="750">
                alt : <a href="../content/documents/PTO_Calendar.pdf">../content/documents/PTO_Calendar.pdf</a>
            </object>
        </div>
    </div>
</div>


